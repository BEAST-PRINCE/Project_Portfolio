{
    "builds": [
      {
        "src": "projectsite/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb", "runtime": "python3.9"
        }
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "projectsite/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "projectsite/wsgi.py"
      }
    ]
  }